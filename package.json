{
  "name": "link-inserter",
  "displayName": "Link Inserter",
  "description": "Helps to insert links in Markdown files.",
  "version": "0.0.1",
  "publisher": "hf-solutions-llc",
  "author": {
    "email": "alcha@hasslefree.solutions",
    "name": "Devin W. Leaman (Alcha)",
    "url": "https://hasslefree.solutions"
  },
  "homepage": "https://github.com/HF-Solutions/Enmeti",
  "activationEvents": [
    "onLanguage:markdown",
    "onCommand:extension.insertLink"
  ],
  "contributes": {
    "commands": [{
      "command": "extension.insertLink",
      "title": "Insert Link",
      "category": "Insertions"
    }]
  },
  "main": "./extension",
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^3.0.1",
    "vscode": "^1.1.18",
    "eslint": "^5.3.0",
    "@types/node": "^10.5.8",
    "@types/mocha": "^5.2.5"
  },
  "engines": {
    "vscode": "^1.25.0"
  },
  "categories": [
    "Other"
  ]
}
